    "main": "dist/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run build:static && npm run build:metadata && npm run build:src",
        "build:static": "copyfiles -f static/*.* dist",
        "build:metadata": "node package-metadata.js",
        "build:src": "tsc -p tsconfig.json",
        "lint": "eslint . --ext .ts,.tsx",
        "start": "node dist",
        "all-the-things": "npm run clean && npm run build && npm run lint"
    },
    "devDependencies": {
        "@types/http-errors": "^1.8.2",
        "@types/node": "^17.0.24",
        "@types/pino": "^7.0.5",
