        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^39.2.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-tsdoc": "^0.2.16",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "fastify": "^3.28.0",
